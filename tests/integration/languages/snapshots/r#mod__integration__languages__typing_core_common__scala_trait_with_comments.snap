trait Animal {
    // Abstract method for animal sound
    def speak(): String
    
    // Default implementation for movement
    def move(): Unit = {
        println("Moving...") // Print movement message
    }
    
    /* Method to get animal info
       Returns a formatted string */
    def info(): String = s"Animal that says: ${speak()}"
}

