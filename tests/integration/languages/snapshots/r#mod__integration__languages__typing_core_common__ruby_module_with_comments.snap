# text_original
module MathUtils
  # Mathematical constants
  PI = 3.14159
  E = 2.71828
  
  # Calculate circle area
  def self.circle_area(radius)
    PI * radius * radius
  end
  
  # Calculate factorial
  def self.factorial(n)
    return 1 if n <= 1
    n * factorial(n - 1) # Recursive call
  end
end

