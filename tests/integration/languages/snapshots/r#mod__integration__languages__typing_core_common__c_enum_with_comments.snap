# text_original
enum color {
    RED,   // Red color
    GREEN, // Green color
    BLUE   // Blue color
};

void print_color(enum color c) {
    switch (c) {
        case RED:
            printf("Red\n");   // Print red
            break;
        case GREEN:
            printf("Green\n"); // Print green
            break;
        case BLUE:
            printf("Blue\n");  // Print blue
            break;
    }
}

