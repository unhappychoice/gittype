# text_original
interface User {
    id: number; // Unique identifier
    name: string; // User's name
    email?: string; // Optional email
}

function createUser(data: Partial<User>): User {
    // Create user with default values
    return {
        id: Math.random(),
        name: data.name || "Unknown",
        ...data
    };
}

