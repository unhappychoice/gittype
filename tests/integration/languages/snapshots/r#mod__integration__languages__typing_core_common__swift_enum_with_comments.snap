# text_original
enum Result<T, E> {
    case success(T) // Success case
    case failure(E) // Failure case
    
    /* Check if result is successful */
    var isSuccess: Bool {
        switch self {
        case .success:
            return true
        case .failure:
            return false
        }
    }
}

